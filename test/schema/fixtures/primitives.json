[
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/booleanElement.schema.json",
    "description": "It is a boolean data element",
    "type": "boolean"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/integerElement.schema.json",
    "description": "It is a integer data element",
    "type": "integer"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/decimalElement.schema.json",
    "description": "It is a decimal data element",
    "type": "number"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/unsignedIntElement.schema.json",
    "description": "It is a unsignedInt data element",
    "type": "integer",
    "minimum": 0
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/positiveIntElement.schema.json",
    "description": "It is a positiveInt data element",
    "type": "integer",
    "minimum": 1
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/stringElement.schema.json",
    "description": "It is a string data element",
    "type": "string"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/markdownElement.schema.json",
    "description": "It is a markdown data element",
    "type": "string"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/codeElement.schema.json",
    "description": "It is a code data element",
    "type": "string",
    "pattern": "[^\\s]+([\\s]?[^\\s]+)*"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/idElement.schema.json",
    "description": "It is a id data element",
    "type": "string",
    "pattern": "[A-Za-z0-9\\-\\.]{1,64}"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/oidElement.schema.json",
    "description": "It is a oid data element",
    "type": "string",
    "pattern": "urn:oid:[0-2](\\.[1-9]\\d*)+"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/uriElement.schema.json",
    "description": "It is a uri data element",
    "type": "string",
    "format": "uri"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/base64BinaryElement.schema.json",
    "description": "It is a base64Binary data element",
    "type": "string"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/dateElement.schema.json",
    "description": "It is a date data element",
    "type": "string",
    "pattern": "-?[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1]))?)?"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/dateTimeElement.schema.json",
    "description": "It is a dateTime data element",
    "type": "string",
    "pattern": "-?[0-9]{4}(-(0[1-9]|1[0-2])(-(0[0-9]|[1-2][0-9]|3[0-1])(T([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\\.[0-9]+)?(Z|(\\+|-)((0[0-9]|1[0-3]):[0-5][0-9]|14:00)))?)?)?"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/instantElement.schema.json",
    "description": "It is a instant data element",
    "type": "string",
    "format": "date-time"
  },
  {
    "$schema": "http://standardhealthrecord.org/schemas/shr/extended-schema.schema.json",
    "id": "http://standardhealthrecord.org/schemas/shr/test/timeElement.schema.json",
    "description": "It is a time data element",
    "type": "string",
    "pattern": "([01][0-9]|2[0-3]):[0-5][0-9]:[0-5][0-9](\\.[0-9]+)?"
  }
]